{
  "cli": {
    "version": ">= 5.2.0",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
